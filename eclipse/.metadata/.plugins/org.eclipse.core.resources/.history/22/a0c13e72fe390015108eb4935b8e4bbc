package com.goku9616.TrollCraftREMAKE.blocks;

import java.util.Random;

import com.goku9616.TrollCraftREMAKE.TrollCraftREMAKE;

import net.minecraft.block.Block;
import net.minecraft.block.material.Material;
import net.minecraft.init.Blocks;
import net.minecraft.item.Item;

public class TrollDiamondOre extends Block{
	public TrollDiamondOre(){
		super(Material.rock);
		setBlockName("TrollDiamondOre");
		setHardness(3F);
		setHarvestLevel("pickaxe", 1);
		this.setBlockTextureName("minecraft:diamond_ore");
	}
	@Override
	public Item getItemDropped(int p_149650_1_, Random rnd, int p_149650_3_)
    {
        return TrollCraftREMAKE.trollDiamond;
    }
}
