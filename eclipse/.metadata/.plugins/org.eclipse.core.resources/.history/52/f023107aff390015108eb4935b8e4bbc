package com.goku9616.TrollCraftREMAKE.blocks;

import java.util.Random;

import com.goku9616.TrollCraftREMAKE.TrollCraftREMAKE;

import net.minecraft.block.Block;
import net.minecraft.block.material.Material;
import net.minecraft.item.Item;

public class TrollTorch extends Block{
	public TrollTorch(){
		super(Material.rock);
		setBlockName("TrollCoalOre");
		setHardness(3F);
		setHarvestLevel("pickaxe", 0);
		this.setBlockTextureName("minecraft:coal_ore");
	}
	@Override
	public Item getItemDropped(int p_149650_1_, Random rnd, int p_149650_3_)
    {
        return TrollCraftREMAKE.trollCoal;
    }
}
