package com.goku9616.TrollCraftREMAKE.blocks;

import net.minecraft.block.Block;
import net.minecraft.block.material.Material;

public class TrollStone extends Block{
	public TrollStone(){
		super(Material.rock);
		setBlockName("TrollStone");
		setBlockUnbreakable();
		this.setBlockTextureName("minecraft:stone");
	}
}
